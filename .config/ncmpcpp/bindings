def_key "j"
  scroll_down
def_key "k"
  scroll_up
def_key "h"
  previous_column
def_key "l"
  next_column
def_key "ctrl-b"
  page_up
def_key "ctrl-u"
  page_up
def_key "ctrl-f"
  page_down
def_key "ctrl-d"
  page_down
def_key "g"
  move_home
def_key "G"
  move_end
def_key "n"
  next_found_item
def_key "N"
  previous_found_item

def_key "space"
  select_item
  scroll_down

def_key "."
  jump_to_playing_song
def_key "L"
  seek_forward
def_key "H"
  seek_backward

# selection
def_key "v"
  select_item
  scroll_down
def_key "V"
  remove_selection

# playlist options
def_key "p" #loads a playlist. you have to remember the name of playlist
  clear_main_playlist
  load
